{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "File": {
      "Path": "Logs/app-log.txt",
      "RollingInterval": "Day"
    }
  },
  "applicationUrl": "http://localhost:5000",
  "AllowedHosts": "*",
  "LogParserSettings": {
    "BaseDir": "SreSandbox/SreSandboxAPI/_logs"
  },
  "ResponderSettings":
  {
    "IPWhitelist": [],
    "IPBlacklist": []
  }
}
